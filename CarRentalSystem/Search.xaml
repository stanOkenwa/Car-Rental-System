<Window x:Class="CarRentalSystem.Search"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarRentalSystem"
        mc:Ignorable="d"
        Title="Search" Height="600" Width="900"  WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" Background="#FF547E76">
    <Grid>
        <TextBox x:Name="txtSearch" HorizontalAlignment="Left" Height="35" Margin="160,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="370" Tag=""/>
        <Button x:Name="btnSearch" Content="Search" HorizontalAlignment="Left" Margin="590,70,0,0" VerticalAlignment="Top" Width="75" Height="30" Click="btnSearch_Click"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="275" Margin="125,145,0,0" VerticalAlignment="Top" Width="600" Visibility="Hidden">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="button" Content="Back" HorizontalAlignment="Left" Margin="60,15,0,0" VerticalAlignment="Top" Width="80" Height="30" Click="button_Click_1"/>
    </Grid>
</Window>
